    <div class="container blog-page">
        <div class="row clearfix">
            <% for(var i=0;i<users.length;i++){ %>
                
              
            <div class="col-lg-4 col-md-12">
                <div class="card single_post">
                    <div class="header">
                    </div>
                    <div class="body" style="margin-right: 50px;">
                        <h3 class="m-t-0 m-b-5"><a href="blog-details.html" style="font-size: 40px;"><%- users[i].username %></a></h3>
                        <ul class="meta">
                            <li><a href="javascript:void(0);" style="font-size: 20px;"><i class="zmdi zmdi-account col-blue"></i><%-users[i].type%></a></li>
                        </ul>
            
                    </div>
                    <div class="body">
                        <div class="img-post m-b-15">
                            <img src="https://bootdey.com/img/Content/avatar/avatar5.png" alt="Awesome Image">
